{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ]
    }
  }
}
